    }
  ],
  "author": "bluefox <dogafox@gmail.com>",
  "contributors": [
    "bluefox <dogafox@gmail.com>"
  ],
  "devDependencies": {
    "gulp": "^3.9.1",
    "npm": "^2.15.12",
    "mocha": "^4.1.0",
    "chai": "^4.1.2",
    "request": "^2.83.0",
    "node.extend": "^2.0.0"
  }
}